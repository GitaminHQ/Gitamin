{
    "name": "gitaminhq/gitamin",
    "description": "An open source git repository management software.",
    "keywords": ["laravel", "gitamin", "git", "repository"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "The Gitamin Team",
            "email": "support@gitamin.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "~5.2.11",
        "phecho/gitlib": "^0.0.4",
        "gitonomy/gitlib": "^1.0.0",
        "gitaminhq/emoji": "^0.1",
        "gitaminhq/markdown": "^0.1",
        "fideloper/proxy": "^3.1",
        "phecho/uploader": "^0.1",
        "alt-three/validator": "^1.4",
        "barryvdh/laravel-cors": "^0.7",
        "doctrine/dbal": "^2.5",
        "graham-campbell/exceptions": "^5.1",
        "guzzlehttp/guzzle": "^6.1",
        "jenssegers/date": "^3.0.13",
        "mccool/laravel-auto-presenter": "^4.2",
        "roumen/feed": "^2.10",
        "laravelcollective/bus": "^2.0@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "gitaminhq/benchtest": "^0.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8.21"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Http/helpers.php"
        ],
        "psr-4": {
            "Gitamin\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gitamin\\Test\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan config:cache",
            "php artisan route:cache",
            "chmod -R 755 storage"
        ],
        "post-update-cmd": [
            "php artisan config:cache",
            "php artisan route:cache",
            "chmod -R 755 storage"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
